{
  "name": "ng-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod --aot --output-hashing=none",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker:build": "docker build -t ng-template/image --label ng-template .",
    "docker:create": "docker create --restart=always -p 5000:5000 --name ng-template ng-template/image:latest",
    "docker": "docker start ng-template",
    "docker:logs": "docker logs ng-template",
    "docker:stop": "docker stop ng-template",
    "docker:rm": "docker rm ng-template",
    "docker:deploy": "npm run docker:build && npm run docker:create && npm run docker",
    "heroku:setup": "heroku stack:set container",
    "heroku:build": "heroku container:login && heroku container:push web --remote heroku",
    "heroku:release": "heroku container:release web --remote heroku",
    "heroku:deploy": "npm run heroku:build && npm run heroku:release && npm run heroku:open",
    "heroku:open": "heroku open --remote heroku"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.10",
    "@angular/common": "~11.2.10",
    "@angular/compiler": "~11.2.10",
    "@angular/core": "~11.2.10",
    "@angular/forms": "~11.2.10",
    "@angular/platform-browser": "~11.2.10",
    "@angular/platform-browser-dynamic": "~11.2.10",
    "@angular/router": "~11.2.10",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.9",
    "@angular/cli": "~11.2.9",
    "@angular/compiler-cli": "~11.2.10",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
